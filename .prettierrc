{
	"semi": false,
	"singleQuote": true,
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"bracketSameLine": true,
	"importOrder": [
		"^react$",
		"<THIRD_PARTY_MODULES>",
		"^@functions/(.*)$",
		"^@models/(.*)$",
		"^@helpers/(.*)$",
		"^@services/(.*)$",
		"^@components/(.*)$",
		"^[./]"
	],
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
